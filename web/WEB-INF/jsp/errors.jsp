<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Errors</title>
</head>
<body>
<c:if test="${not empty requestScope.errors}">
    <div style="color: red">
        <c:forEach var="error" items="${requestScope.errors}">
            <span>${error.message}</span>
            <br>
        </c:forEach>
    </div>
</c:if>
</body>
</html>
